{
    "name": "0xscanner",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "prepare": "husky install",
        "tsc": "tsc",
        "dev": "set NODE_PATH=src/&& dotenv -e .env ts-node-dev --respawn --experimental-modules --transpile-only --watch src ./src/index.ts",
        "dev-build": "tsc && set NODE_PATH=build/&& dotenv -e .env.development node ./build/index.js",
        "dev-build-linux": "tsc && NODE_PATH=build/ dotenv -e .env.development node ./build/index.js",
        "dev-linux": "tsc && NODE_PATH=build/ dotenv -e .env.dev node ./build/index.js",
        "prod": "set NODE_PATH=build/&& dotenv -e .env.production node ./build/index.js",
        "prod-linux": "NODE_PATH=build/ dotenv -e .env node ./build/index.js",
        "lint": "eslint src --fix",
        "prettier:fix": "prettier --write",
        "prettier:check": "prettier --check"
    },
    "dependencies": {
        "@solana/spl-token": "^0.4.3",
        "@solana/web3.js": "^1.91.4",
        "axios": "^1.6.5",
        "body-parser": "^1.20.2",
        "bs58": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.2",
        "ethers": "^5.7.2",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-swagger-generator": "^1.1.17",
        "mysql2": "^3.7.1",
        "node-telegram-bot-api": "^0.65.1",
        "sequelize": "^6.35.2",
        "web3": "^3.0.0-rc.5"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node-telegram-bot-api": "^0.64.6",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "dotenv-cli": "^7.4.1",
        "eslint": "^8.56.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^8.0.3",
        "prettier": "^3.2.4",
        "sequelize-auto": "^0.8.8",
        "sequelize-cli": "^6.6.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}
